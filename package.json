    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "axios": "^1.2.6",
    "eslint": "8.31.0",
    "eslint-config-next": "12.3.4",
    "formik": "^2.2.9",
    "moment": "^2.29.4",
    "next": "12.3.4",
    "next-themes": "^0.2.1",
    "plaiceholder": "^2.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scrolllock": "^5.0.1",
    "react-share": "^4.4.1",
    "react-toastify": "^9.1.1",
